{
    "rulesDirectory": [ "../node_modules/codelyzer" ],
    "extends": ["tslint:recommended", "tslint-angular","tslint-config-prettier"],
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
